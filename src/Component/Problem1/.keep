const Sum_to_n_a = (n: number) => {
  let sum = 0;
  for (let i = 1; i <= Math.abs(n); i++) {
    sum += i;
  }
  return sum;
};

const Sum_to_n_b = (n: number): number => {
  return (Math.abs(n) * (Math.abs(n) + 1)) / 2;
};

const Sum_to_n_c = (n: number): number => {
  let sum = 0;
  let i = 1;
  while (i <= Math.abs(n)) {
    sum += i;
    i++;
  }
  return sum;
};
